bed2623dc2cf19c2830165389bfb4ba6  data/config.site
SHAR_EOF

else
test `LC_ALL=C wc -c < 'data/config.site'` -ne 620 && \
  ${echo} "restoration warning:  size of 'data/config.site' is not 620"
  fi
fi
