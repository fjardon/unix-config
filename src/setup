    ${INSTALL_X} scripts/msvc-shell "${PREFIX}/.local/bin"
    ${INSTALL_R} man/msvc-shell.1   "${PREFIX}/.local/share/man/man1"
    ${INSTALL_X} apt-cyg            "${PREFIX}/.local/bin"
fi
