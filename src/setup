#   3231 -rw-r--r-- man/byzanz-helper.1
#   2786 -rw-r--r-- man/codefmt.1
#   2844 -rw-r--r-- man/codemv.1
#   4364 -rw-r--r-- man/plgen.1
#   3187 -rw-r--r-- man/ffmpeg-helper.1
#   2643 -rw-r--r-- man/hyper-v.1
#   3402 -rw-r--r-- man/msvc-shell.1
#   3947 -rw-r--r-- man/sixel2tmux.1
#   4604 -rw-r--r-- man/yank.1
