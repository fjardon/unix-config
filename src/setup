if test $? -ne 0
then ${echo} "restore of dot/bash_profile failed"
fi
  if ${md5check}
  then (
       ${MD5SUM} -c >/dev/null 2>&1 || ${echo} 'dot/bash_profile': 'MD5 check failed'
       ) << \SHAR_EOF
d08cc8151076078a1ab759db11f5ee3e  dot/bash_profile
SHAR_EOF

else
test `LC_ALL=C wc -c < 'dot/bash_profile'` -ne 475 && \
  ${echo} "restoration warning:  size of 'dot/bash_profile' is not 475"
  fi
fi
